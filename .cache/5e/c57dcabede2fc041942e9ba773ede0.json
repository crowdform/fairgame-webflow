crosshairs;\n    position:absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    width: 100%;\n    height:100%;\n    appearance: none;\n  &:focus {\n    outline: none;\n    appearance: none;\n  }\n}\n\na{\n  color:red;\n  text-decoration: none; \n}\n\n\n.game-header{\n  padding:20px;\n  font-size:30px;\n  font-weight:bolder;\n}\n\n.game-header::after {\n  content: counter(hit);\n}\n\n.fg-crosshairs {\n    cursor: url('./assets/fg-cursor.png') 44 44, auto;\n}\n.game-row{\nposition:absolute;\n    bottom: 0;\n    left: 0;\n    right: 0;\n  width:100vw;\n  height:200px;\n  // border-bottom: 5px solid brown;\n  overflow: hidden;\n  perspective: 1000px;\n    align-items: flex-end;\n}\n\n.duck-row{\n    @extend .fg-crosshairs;\n  display:flex;\n  flex-wrap:nowrap;\n       overflow: hidden;\n\n//   margin-left: -100vw;\n  transform: translate3d(-100vw, 0, 0);\n  width:100vw;\n  height:200px;\n  transform-style: preserve-3d;\n  animation: move 10s linear forwards 1;\n  animation-iteration-count: infinite;\n      align-items: flex-end;\n//   &--two{\n//     margin-left: 2000px;\n//     animation: move2 15s linear forwards .3s 1;\n//   }\n//   &--three{\n//     animation: move 14s linear forwards 3s 1;\n//   }\n  &__box{\n    // margin-top:50px;\n    // margin-right:100px;\n    @extend .fg-crosshairs;\n    width: 72px;\n    height: 80.85px;\n    position: relative;\n \n    // &--reverse{\n    //   transform: rotateY(180deg);\n    // }\n  }\n  &__img{\n    @extend .fg-crosshairs;\n    width: 72px;\n    height: 80.85px;\n    // margin-top: -150px;\n    background:url('./assets/fg-duck.png');\n    background-size:contain;\n    transition:all .2s;\n  }\n}\n\n\n// .game-over{\n//   position:absolute;\n//   top:100px;\n//   right:0;\n//   left:0;\n//   margin-right: auto;\n//   margin-left: auto;\n//   width:800px;\n//   height:479px;\n//   background:white;\n//   font-size:30px;\n//   background:url(https://s-media-cache-ak0.pinimg.com/736x/d9/a7/fc/d9a7fc470357f881adb27b0ed507f94d.jpg), red;\n//   background-size:cover;\n//   background-repeat:no-repeat;\n//   .game-txt{\n//     position:absolute;\n//     top:190px;\n//     width:100%;\n//     height:60px;\n//     h1{\n//       font-size:70px;\n//     }\n//   }\n// }\n\n// .game-score::after {\n//   content: counter(hit);\n// }\n\n\n\n/************* \nStates\n**************/\n\ninput:checked {\n  counter-increment: hit;\n  &:after{\n    content:\"BANG!\";\n    position:relative;\n    top: 10px; \n    left:0;\n    right: 0;\n    padding:10px;\n    width:75px;\n    height:75px;\n    z-index:5;\n    color: rgb(239, 196, 0);\n    font-size:12px;\n    // background:rgba(black, .6);\n    // border-radius:50%;\n    animation: hit 2s forwards;\n    \n  }\n  & ~.duck-row__img{\n    // background:url(http://cliparts.co/cliparts/zcX/oqo/zcXoqorgi.png);\n    // background-size:contain;\n    background: none;\n    &:after{\n      content:\"\";\n      position:absolute;\n      top:20;\n      left:500;\n      width:inherit;\n      height:inherit;\n    }\n  }\n}\n\n\n\n@keyframes move {\n  0%{\n    //   margin-left: -4000px;\n    transform: translate3d(-100vw, 0, 0);\n    }\n  100%{\n    // margin-left:2500px;\n   transform: translate3d(100vw, 0, 0);\n   }\n}\n\n// @keyframes move2 {\n//   0%{margin-left: 2000px;}\n//   100%{margin-left:-4000px;}\n// }\n\n\n@keyframes flow{\n  0%{left:80px;}\n  50%{left:65px}\n}\n\n@keyframes hit{\n  0%{top: 10px;}\n  100%{\n    top:-100px;\n    display: none;\n    opacity: 0;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"3fc3adb372bd01a1654d235e4c033b99","cacheData":{"env":{}}}